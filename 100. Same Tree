//100. Same Tree 
    
    bool solve(TreeNode* p, TreeNode* q)
    {
        if(p==nullptr || q==nullptr)
        {
          if(p==nullptr && q==nullptr) return true;
          else return false;
        }
        
        if(p->val == q->val)
        {
           return solve(p->left,q->left) && solve(p->right,q->right);
        }
        
        return false;  
    }
    bool isSameTree(TreeNode* p, TreeNode* q) 
    {
        return solve(p,q);
    }
