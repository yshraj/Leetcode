    int hammingWeight(uint32_t n)
    {
        int c=0;
        for(int i=0;i<32;i++)
        {
            
            if(n%2!=0)
            {
                c++;
            }
            // Or other logic
            // if(n&1!=0)
            // {
            //     c++;
            // }
             n = n>>1;
        }
        return c;
    }
